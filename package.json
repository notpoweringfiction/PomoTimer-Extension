{
  "name": "simple-pomodoro",
  "displayName": "Simple Pomodoro Timer",
  "version": "1.0.0",
  "engines": { "vscode": "^1.60.0" },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "contributes": {
    "commands": [{
      "command": "pomodoro.toggle",
      "title": "Toggle Pomodoro Timer"
    },
    {
      "command": "pomodoro.setTime",
      "title": "Set Pomodoro Timer"
    }
    ]
  }
}